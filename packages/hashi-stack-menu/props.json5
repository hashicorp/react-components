{
  items: {
    type: 'json',
    required: true,
    description: 'The array of nav items and their applicable browse panel data to be rendered',
  },
  onPanelChange: {
    type: 'function',
    required: false,
    description: 'An optional callback function run on change of the active panel state',
  },
  Link: {
    type: 'component',
    required: false,
    description: "An optional prop to pass `next/link` for adding clientside navigation to the site's index page; Link will wrap and anchor tag on the Logo",
  },
}
